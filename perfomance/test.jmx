            <stringProp name="BeanShellAssertion.query">String balance_string = vars.get(&quot;balance&quot;);
int thread = ctx.getThreadNum();

Double balance = Double.valueOf(balance_string);

// от первого юзера деньги только уходят, поэтому -50.0 от 100
if(thread == 0) {
	if(balance == 51D)
	{
		Failure = false;
	}
	else {
		Failure = true;
		FailureMessage = &quot;balance: &quot; + balance + &quot;, thread: &quot; + thread;
	}
}
// последнему юзеру деньги только приходят, поэтому +50.0 к 200
else if(thread == 99) {
	if(balance == 250D)
	{
		Failure = false;
	}
	else {
		Failure = true;
		FailureMessage = &quot;balance: &quot; + balance + &quot;, thread: &quot; + thread;
	}
}
// у остальных баланс по итогам переводов должен остаться неизменным
else {
	if(balance == ((double)(100 + thread + 1)))
	{
		Failure = false;
	}
	else {
		Failure = true;
		FailureMessage = &quot;balance: &quot; + balance + &quot;, thread: &quot; + thread;
	}
}
</stringProp>
